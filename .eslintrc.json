{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "ignorePatterns":[
        "App.test.js",
        "reportWebVitals.js",
        "setupTest.js"
    ],
    "rules": {
        "require-jsdoc":0,
        "linebreak-style":0,
        "react/prop-types":0,
        "no-unused-vars":0,
        "react/no-unescaped-entities":0
    }
}
